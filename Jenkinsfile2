pipeline{
	agent any
	stages{
		stage("Step 1"){
			steps{
				echo "This is build number $BUILD_NUMBER"
				
			}
		}
		stage("Script"){
			steps{
				ssh '''
					echo "Executting from inside ssh block"
					chmod -x test.sh
					./test.sh
				'''
			}
		}
	}
	environment{
		MY_NAME = "Alex Petrushchak"
	}
  agent any
    stages{
      stage("WhatIsUp"){
        steps{
          echo "This is build number $BUILD_NUMBER by $MY_NAME"
        }
     }
  }
  environment{
      MY_NAME = "Alex Petrushchak"
  }
}
